{
  "name": "@ryanbethel/e-components",
  "type": "module",
  "description": "Enhance wrapper for Mdash. Mdash is a design system based on web standards. It's extremely small (see comparison chart), fast, simple, and compatible with any JavaScript framework. \n",
  "version": "0.0.18",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanbethel/e-components"
  },
  "bugs": "https://github.com/ryanbethel/e-components/issues",
  "main": "index.js",
  "files": [
    "index.js",
    "elements.js",
    "components.js",
    "/app/elements/e/",
    "/app/components/",
    "/public/e-global-custom-properties.css",
    "/public/e-global-basic-reset.css",
    "paramour-config.mjs"
  ],
  "license": "MIT",
  "contributors": [
    {
      "name": "Ryan Bethel",
      "url": "https://github.com/ryanbethel/e-components"
    }
  ],
  "keywords": [
    "ui",
    "components",
    "web",
    "css",
    "html",
    "frontend",
    "webcomponents",
    "customelements",
    "enhance"
  ],
  "scripts": {
    "prepublish": "npm run wrapComponents",
    "start": "npm run wrapComponents && enhance dev",
    "wrapComponents": "node ./scripts/componentWrapper.mjs"
  },
  "devDependencies": {
    "@begin/cli": "^0.19.0",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.2",
    "@enhance/arc-plugin-enhance": "^11.0.1",
    "@paramour/arc-plugin-paramour-css": "^1.0.0-rc.1",
    "@enhance/cli": "^1.1.0",
    "@enhance/styles-cribsheet": "^0.0.11",
    "codemirror": "^6.0.1"
  },
  "dependencies": {
    "@enhance/custom-element": "^1.2.3-next.0"
  }
}
