{
  "name": "@ryanbethel/e-components",
  "type": "module",
  "description": "Enhance wrapper for Mdash. Mdash is a design system based on web standards. It's extremely small (see comparison chart), fast, simple, and compatible with any JavaScript framework. \n",
  "version": "0.0.14",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanbethel/e-components"
  },
  "bugs": "https://github.com/ryanbethel/e-components/issues",
  "main": "index.js",
  "files": [
    "index.js",
    "/elements",
    "/helpers",
    "/assets"
  ],
  "license": "MIT",
  "contributors": [
    {
      "name": "Ryan Bethel",
      "url": "https://github.com/ryanbethel/e-components"
    }
  ],
  "keywords": [
    "ui",
    "components",
    "web",
    "css",
    "html",
    "frontend",
    "webcomponents",
    "customelements",
    "enhance"
  ],
  "scripts": {
    "prepublish": "npm run wrapComponents",
    "start": "npm run wrapComponents && cd example && enhance dev",
    "wrapComponents": "node ./scripts/componentWrapper.mjs"
  },
  "devDependencies": {
    "@enhance/cli": "^1.1.0"
  },
  "dependencies": {
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.2",
    "@enhance/arc-plugin-enhance": "^11.0.1",
    "@enhance/custom-element": "^1.2.3",
    "@enhance/custom-element-mixin": "github:enhance-dev/enhance-custom-element-mixin#dom-diffing-slots",
    "codemirror": "^6.0.1",
    "enhance-idiomorph-mixin": "github:kristoferjoseph/enhance-idiomorph-mixin",
    "morphdom": "^2.7.2",
    "@enhance/arc-plugin-styles": "^5.0.5",
    "@enhance/cli": "^1.1.0",
    "@enhance/styles-cribsheet": "^0.0.11"
  }
}
